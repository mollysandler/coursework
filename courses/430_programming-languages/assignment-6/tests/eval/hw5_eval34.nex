fun f(g : (-> none)) : none
{
   var b : int;
   var c : int;
   b = 100;
   c = 200;
   g();
   print (b);
   print (":n");
   print (c);
   print (":n");
}

fun h() : none
{
   var b : int;
   var c : int;
   b = 1;
   c = 2;
   f(anon () : none { b = b * 2; c = c + 7;});
   print (b);
   print (":n");
   print (c);
   print (":n");
}

h();
