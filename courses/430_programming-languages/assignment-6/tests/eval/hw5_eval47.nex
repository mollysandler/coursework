var factory : (int -> (-> int));
var plus1 : (-> int);
var plus10 : (-> int);
fun f() : (int -> (-> int))
{
   var shared : int;
   shared = 0;

   return anon(x : int) : (-> int) {
      return anon() : int {
         shared = shared + x;
         return shared;
      };
   };
}

factory = f();
plus1 = factory(1);
plus10 = factory(10);

plus1();
plus1();
plus10();
print (plus1());
print ":n";
